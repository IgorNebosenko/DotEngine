<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{DynamicResource PanelBackgroundColor}"/>
    <SolidColorBrush x:Key="PanelBorderBrush" Color="{DynamicResource PanelBorderColor}"/>
    <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="{DynamicResource HeaderBackgroundColor}"/>
    <SolidColorBrush x:Key="HeaderForegroundBrush" Color="{DynamicResource HeaderForegroundColor}"/>
    <SolidColorBrush x:Key="TreeBackgroundBrush" Color="{DynamicResource TreeBackgroundColor}"/>
    <SolidColorBrush x:Key="TreeForegroundBrush" Color="{DynamicResource TreeForegroundColor}"/>

</ResourceDictionary>