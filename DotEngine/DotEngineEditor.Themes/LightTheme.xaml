<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#FFFFFFFF</Color>
    <Color x:Key="ForegroundColor">#FF000000</Color>
    <Color x:Key="ButtonColor">#FFFFFFFF</Color>
    <Color x:Key="MenuItemColor">#FFFFFFFF</Color>
    <Color x:Key="ToolbarButtonHoverColor">Black</Color>
    <Color x:Key="PanelBackgroundColor">#FFF8F8F8</Color>
    <Color x:Key="PanelBorderColor">#CCCCCC</Color>
    <Color x:Key="HeaderBackgroundColor">#FFEFEFEF</Color>
    <Color x:Key="HeaderForegroundColor">#FF000000</Color>
    <Color x:Key="TreeBackgroundColor">#FFFFFFFF</Color>
    <Color x:Key="TreeForegroundColor">#FF000000</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{StaticResource PanelBackgroundColor}"/>
    <SolidColorBrush x:Key="PanelBorderBrush" Color="{StaticResource PanelBorderColor}"/>

    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="#FFF6F6F6"/>
    <SolidColorBrush x:Key="MenuForegroundBrush" Color="#222222"/>
    <SolidColorBrush x:Key="MenuHoverBrush" Color="#FFE0E0E0"/>
    <SolidColorBrush x:Key="MenuHoverTextBrush" Color="#000000"/>
    <SolidColorBrush x:Key="MenuDisabledTextBrush" Color="#999999"/>
    <SolidColorBrush x:Key="MenuGestureBrush" Color="#777777"/>
    <SolidColorBrush x:Key="MenuBorderBrush" Color="#FFCCCCCC"/>

    <SolidColorBrush x:Key="SubMenuBackgroundBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="SubMenuForegroundBrush" Color="#222222"/>
    <SolidColorBrush x:Key="SubMenuArrowBrush" Color="#222222"/>

    <SolidColorBrush x:Key="IconColorBrush" Color="#FF2D2D30"/>
    <SolidColorBrush x:Key="IconBrush" Color="#FF000000"/>

    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="ButtonPrimaryBackgroundBrush" Color="#3A5BD9"/>
    <SolidColorBrush x:Key="ButtonDangerBackgroundBrush" Color="#E57373"/>

    <SolidColorBrush x:Key="ConsoleInfoBrush" Color="#303030"/>
    <SolidColorBrush x:Key="ConsoleWarnBrush" Color="#C07A00"/>
    <SolidColorBrush x:Key="ConsoleErrorBrush" Color="#D83A3A"/>

    <Style TargetType="Window">
        <Setter Property="Background" Value="{StaticResource BackgroundBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource ForegroundBrush}"/>
    </Style>

    <Style TargetType="Button">
        <Setter Property="Background" Value="{StaticResource ButtonColor}"/>
        <Setter Property="Foreground" Value="{StaticResource ForegroundColor}"/>
    </Style>
</ResourceDictionary>
