<Application x:Class="DotEngineEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DotEngineEditor.Themes;component/LightTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/DotEngineEditor.Themes;component/DarkTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/DotEngineEditor.Themes;component/AvalonDockTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>