<Window x:Class="DotEngineEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DotEngineEditor"
        xmlns:userControls="clr-namespace:DotEngineEditor.UserControls;assembly=DotEngineEditor.UserControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200">
    <DockPanel 
        Background="{DynamicResource BackgroundBrush}"
        LastChildFill="True">
        <userControls:MenuBar x:Name="MenuBarItem" DockPanel.Dock="Top"/>
        <Button Content="Test"
                Width="120" Height="34"
                Click="OnTestButtonClick"
                Background="{DynamicResource BackgroundBrush}"
                Foreground="{DynamicResource ForegroundBrush}"/>
    </DockPanel>
</Window>
